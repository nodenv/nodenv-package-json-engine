{
  "name": "@nodenv/nodenv-package-json-engine",
  "version": "3.0.3",
  "description": "A nodenv plugin to activate Node version from the package.json#engines.node field.",
  "homepage": "https://github.com/nodenv/nodenv-package-json-engine#readme",
  "license": "MIT",
  "author": "Adam Hull (http://hurrymaplelad.com)",
  "contributors": [
    "Jason Karns <jason.karns@gmail.com> (http://jasonkarns.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodenv/nodenv-package-json-engine.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-package-json-engine/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "files": [
    "bin",
    "etc",
    "libexec"
  ],
  "scripts": {
    "test": "bats ${CI:+--tap} test",
    "posttest": "npm run lint",
    "lint": "git ls-files bin etc libexec test/*.bash | grep -Ev '(semver|JSON).sh' | xargs shellcheck",
    "postlint": "prettier --check .",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@nodenv/nodenv": "^1.6.2",
    "bats": "^1.13.0",
    "bats-assert": "^2.2.4",
    "bats-support": "^0.3.0",
    "prettier": "^3.7.3",
    "shellcheck": "^4.1.0"
  },
  "dependencies": {
    "JSON.sh": "~0.3.3",
    "sh-semver": "github:qzb/sh-semver"
  }
}
